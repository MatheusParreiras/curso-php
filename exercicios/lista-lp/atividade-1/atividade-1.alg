algoritmo "Atividade 1 da Apostila LP"
// Função :
// Autor :
// Data : 08/03/2023
// Seção de Declarações 
var
a , b , aux : inteiro
inicio
escreval("Valores antes da troca")
a := 10
b := 20

escreval("A = ", a)
escreval("B = ", b)

aux := b
b := a
a := aux

escreval("Valores depois da troca")
escreval("A = ", a)
escreval("B = ", b)
// Seção de Comandos 
fimalgoritmo
