// Seção de Declarações 
var
x , y , z : inteiro
inicio

x := 1
y := 2
z := (y - x)

escreval("Antigo valor de Z")
escreval("Z = ", z)

x := 5
y := (x + z)

escreval ("Valores de X, Y e Z")
escreval("X = ", x)
escreval("Y = ", y)
escreval("Z = ", z)


// Seção de Comandos 
fimalgoritmo
